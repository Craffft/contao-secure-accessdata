{
    "name": "craffft/contao-secure-accessdata",
    "description": "Secure Accessdata for Contao OpenSource CMS",
    "keywords": ["secure", "access", "data", "password", "encryption"],
    "type": "contao-module",
    "homepage": "http://craffft.de",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Daniel Kiesel",
            "email": "daniel@craffft.de",
            "homepage": "http://craffft.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Craffft/contao-secure-accessdata/issues",
        "source": "https://github.com/Craffft/contao-secure-accessdata"
    },
    "require": {
        "php": ">=5.3.2",
        "contao/core": ">=3.2,<3.5-dev",
        "contao-community-alliance/composer-installer": "*"
    },
    "autoload": {
        "classmap": ["system/"]
    },
    "replace": {
        "contao-legacy/secure_accessdata": "*"
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/secure-accessdata": "system/modules/secure-accessdata"
            }
        }
    }
}
